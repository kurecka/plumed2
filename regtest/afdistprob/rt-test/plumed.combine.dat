d: DISTANCE ATOMS=1,5
exp1: CUSTOM ARG=d FUNC=exp(-100*(x-0.110)^2) PERIODIC=NO
exp2: CUSTOM ARG=d FUNC=exp(-100*(x-0.111)^2) PERIODIC=NO
exp3: CUSTOM ARG=d FUNC=exp(-100*(x-0.112)^2) PERIODIC=NO
exp4: CUSTOM ARG=d FUNC=exp(-100*(x-0.113)^2) PERIODIC=NO

num: COMBINE ARG=exp1,exp2,exp3,exp4 COEFFICIENTS=0.1,0.4,0.3,0.2 PERIODIC=NO
den: COMBINE ARG=exp1,exp2,exp3,exp4 PERIODIC=NO
prp: CUSTOM ARG=num,den FUNC=x/y PERIODIC=NO

PRINT ARG=exp1,exp2,exp3,exp4 STRIDE=1 FILE=colvar FMT=%15.4f
